<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.aplicacion.envivoapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".utilidades.MyFirebaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_img_principal"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_img_principal_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EnVivoApp">
        <activity android:name=".activityParaClientes.HomeCliente"></activity>
        <activity android:name=".activitysParaVendedores.HomeVendedor" />
        <activity android:name=".Reportes.ReporteVendedor" />
        <activity android:name=".activitysParaVendedores.MensajeriaGlobalVendedor" />
        <activity android:name=".activitysParaVendedores.ListarClientes" />
        <activity android:name=".activityParaClientes.MensajeriaGlobal" />
        <activity android:name=".activitysParaVendedores.PedidoVendedor" />
        <activity android:name=".activityParaClientes.PedidoCliente" />
        <activity android:name=".activitysParaVendedores.MensajeriaVendedor" />
        <activity android:name=".activityParaClientes.ListarStreamingsVendedor" />
        <activity android:name=".activityParaClientes.MensajeriaCliente" />
        <activity
            android:name=".activityParaClientes.ListarVendedores"
            android:label="@string/title_activity_listar_vendedores"
            android:theme="@style/Theme.EnVivoApp.NoActionBar" />
        <activity
            android:name=".activityParaClientes.DataCliente"
            android:label="@string/title_activity_data_cliente"
            android:theme="@style/Theme.EnVivoApp.NoActionBar" />
        <activity
            android:name=".activitysParaVendedores.GestionVideos"
            android:label="@string/title_activity_gestion_videos"
            android:theme="@style/Theme.EnVivoApp.NoActionBar" />
        <activity
            android:name=".activitysParaVendedores.DataLocal"
            android:label="@string/title_activity_data_local"
            android:theme="@style/Theme.EnVivoApp.NoActionBar" />
        <activity
            android:name=".activitysParaVendedores.DataVendedor"
            android:label="@string/title_activity_data_vendedor"
            android:theme="@style/Theme.EnVivoApp.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.EnVivoApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            tools:replace="android:value"
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
    </application>

</manifest>