<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.EnVivoApp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00a7c7</item>
        <item name="colorPrimaryVariant">#000000</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <item name="colorOnSurface">#feab6b</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#f3e9e7</item>
        <item name="colorSecondaryVariant">#9bcfe0</item>
        <item name="colorOnSecondary">#e76f3d</item>e76f3d
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.EnVivoApp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.EnVivoApp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.EnVivoApp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>